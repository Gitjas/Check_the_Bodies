  IF ~Global("CbTalkedGaelan","GLOBAL",0)~ THEN GOTO 68
END

EXTEND_BOTTOM GAELAN 67
  IF ~Global("CbTalkedGaelan","GLOBAL",1)~ THEN GOTO CB_90
END

EXTEND_BOTTOM GAELAN 67 
  IF ~Global("CbTalkedGaelan","GLOBAL",2)~ THEN GOTO 69
END

ADD_TRANS_TRIGGER GAELAN 68
~False()~

