  IF ~~ THEN DO ~SetGlobal("CbTalkedGaelan","GLOBAL",3)
EscapeArea()~ EXIT
END

APPEND GAELAN
IF ~~ THEN BEGIN CB_90
  SAY @178
  IF ~~ THEN DO ~SetGlobal("CbTalkedGaelan","GLOBAL",2)~ EXIT
END
END
