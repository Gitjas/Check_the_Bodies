		SetGlobal("Chapter","GLOBAL",1)
		ActionOverride("Malaaq",MoveBetweenAreas("AR0601",[345.591],14))
		ActionOverride("DuegarClanChief",TakeItemListPartyNum("IMPORT01",1))
		ActionOverride("Shelf1",TakeItemListPartyNum("IMPORT03",1))
