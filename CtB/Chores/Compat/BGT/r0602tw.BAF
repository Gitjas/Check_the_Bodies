IF
	Global("TakeImportItems","AR0602",0)
        Global("MinscImport","GLOBAL",2)
        Global("JaheiraImport","GLOBAL",2)
        Global("ImoenImport","GLOBAL",2)
	Gender(Player1,MALE)
THEN
	RESPONSE #100
